{
    "compilerOptions": {
        "target": "es2022",
        "module": "es2022",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "allowJs": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "rootDir": "./webapp",
        "baseUrl": "./",
        "paths": {
            "ui5/gebit/app/*": ["webapp/*"]
        },
        "composite": true
    },
    "include": ["./webapp/**/*"],
    "typeRoots": [
        "./node_modules/@types",
        "./node_modules/@sapui5/types"
    ]

    
}