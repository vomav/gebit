{
	"_version": "1.21.0",
	"sap.app": {
		"id": "ui5.gebit.app",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				"en",
				"de",
				"uk",
				"ru",
				"fr"
			],
			"fallbackLocale": "en"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",

		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"userAdminModel": {
				"uri": "/odata/v4/srv.admin/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			},
			"mainService": {
				"uri": "/odata/v4/srv.searching/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			},
			"publicService": {
				"uri": "/odata/v4/srv.publicSearching/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"icons": {
			"icon": "icon.png",
			"favIcon": "favicon.ico",
			"phone": "icon.png",
			"tablet": "icon.png"
		}
	},
	"sap.ui5": {
		"contentDensities": {
			"cozy": true,
			"compact": true
		},
		"rootView": {
			"viewName": "ui5.gebit.app.view.App",
			"type": "XML",
			"id": "app"
		},
		"componentUsages": {
			"publicComponent": {
				"name": "ui5.gebit.app.reuse.public",
				"settings": {},
				"componentData": {},
				"lazy": true
			},
			"siteComponent": {
				"name": "ui5.gebit.app.reuse.sites",
				"settings": {},
				"componentData": {},
				"lazy": true
			},
			"adminComponent": {
				"name": "ui5.gebit.app.reuse.admin",
				"settings": {},
				"componentData": {},
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.112.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {},
				"sap.f": {},
				"sap.tnt": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.gebit.app.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"uiModel": {
				"type": "sap.ui.model.json.JSONModel",
				    "preload": true,
    				"uri": "model/uiModel.json"
			  },
			"": {
				"dataSource": "mainService",
				"preload": false,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"autoExpandSelect": true,
					"earlyRequests": false,
					"operationMode": "Server",
					"groupId": "$direct"
				}
			},
			"userAdminModel": {
				"dataSource": "userAdminModel",
				"preload": false,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"autoExpandSelect": true,
					"earlyRequests": false,
					"operationMode": "Server",
					"groupId": "$direct"
				}
			},
			"publicService": {
				"dataSource": "publicService",
				"preload": false,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"autoExpandSelect": true,
					"earlyRequests": false,
					"operationMode": "Server",
					"groupId": "$direct"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "ui5.gebit.app.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true
			},
			"routes": [
				
				{
					"name": "home",
					"pattern": "",
					"target": "home"
				},
				{
					"name": "welcome",
					"pattern": "welcome",
					"target": {
						"name": "welcome"
					}
				},
				{
					"name": "territories",
					"pattern": "territories",
					"target": {
						"name": "territories"
					}
				},
				{
					"name": "territoryDetail",
					"pattern": "territory/{id}",
					"target": "territoryDetail"
				},
				{
					"name": "myTerritories",
					"pattern": "myTerritories",
					"target": {
						"name": "myTerritories"
					}
				},
				{
					"name": "myTerritoryDetail",
					"pattern": "myTerritory/{id}",
					"target": "myTerritoryDetail"
				},
				{
					"name": "groupTerritories",
					"pattern": "groupTerritories",
					"target": {
						"name": "groupTerritories"
					}
				},
				{
					"name": "groupTerritoryDetail",
					"pattern": "groupTerritoryDetail/{id}",
					"target": "groupTerritoryDetail"
				},
				{
					"name": "sites",
					"pattern": "sites",
					"target": {
						"name": "sites"
					}
				},
				{
					"name": "groupTerritoryDetail",
					"pattern": "groupTerritoryDetail/{id}",
					"target": "groupTerritoryDetail"
				},
				{
					"name": "userAdmin",
					"pattern": "userAdmin",
					"target": "userAdmin"
				},
				{
					"name": "videoTutorial",
					"pattern": "videoTutorial",
					"target": "videoTutorial"
				},
				{
					"name": "publicSearching",
					"pattern": "publicSearching/{id}",
					"target": "publicSearching"
				},
				{
					"name": "admin",
					"pattern": "admin",
					"target": "admin"
				}
			],
			"targets": {
				"home": {
					"type": "View",
					"id": "home",
					"name": "Home"
				},
				"welcome": {
					"type": "Component",
					"usage": "publicComponent",
					"id": "publicComponent"
				},
				"territories": {
					"type": "View",
					"id": "territoryWorklist",
					"name": "TerritoryWorklist"
				},
				"territoryDetail": {
					"type": "View",
					"id": "territoryDetail",
					"name": "TerritoryDetail"
				},
				"myTerritories": {
					"type": "View",
					"id": "myTerritoryWorklist",
					"name": "MyTerritoryWorklist"
				},
				"myTerritoryDetail": {
					"type": "View",
					"id": "myTerritoryDetail",
					"name": "MyTerritoryDetail"
				},
				"groupTerritories": {
					"type": "View",
					"id": "groupTerritoriesWorklist",
					"name": "GroupTerritoriesWorklist"
				},
				"groupTerritoryDetail": {
					"type": "View",
					"id": "groupTerritoryDetail",
					"name": "GroupTerritoryDetail"
				},
				"sites": {
					"type": "Component",
					"usage": "siteComponent",
					"id": "siteComponent"
				},
				"userAdmin": {
					"type": "View",
					"id": "userAdmin",
					"name": "UserAdmin"
				},
				"videoTutorial": {
					"type": "View",
					"id": "videoTutorial",
					"name": "VideoTutorial"
				},
				"publicSearching": {
					"type": "View",
					"id": "publicSearching",
					"name": "PublicTerritory"
				},
				"admin": {
					"type": "Component",
					"usage": "adminComponent",
					"id": "adminComponent"
				}
			}
		}
	}
}
