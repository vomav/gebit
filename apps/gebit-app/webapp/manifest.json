{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5.gebit.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "UI5 TypeScript Hello World",
		"description": "UI5 TypeScript Hello World",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "set-ach",
		"dataSources": {
			"Data.svc": {
			  "uri": "../Data.svc?sap-context-token=1", 
			  "type": "OData",
			  "settings": {
				"odataVersion": "4.0"
			  }
			}
		  }
	},

	"sap.fiori": {

	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5.gebit.app.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.104.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.integration": {},
				"sap.uxap": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				  "bundleName": "ui5.gebit.app.i18n.i18n"
				}
			  }

		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "ui5.gebit.app.App.view",
				"transition": "slide",
				"controlId": "fcl",
				"async": true
			  }
		}
	}
}