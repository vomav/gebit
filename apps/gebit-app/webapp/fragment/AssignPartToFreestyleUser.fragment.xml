<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   xmlns:form="sap.ui.layout.form">
   	<Dialog
		title="{i18n>unregisteredUserDialogTitle}">
		<Input id="assignFreestyleName"  placeholder="{i18n>writeTheNameOfUnregisteredUser}" />
		<endButton>
			<Button text="{i18n>assignUnregisteredUserDialogButton}" press="onAddUnregisteredUser"></Button>
		</endButton>
		<beginButton>
			<Button text="{i18n>cancel}" press="closeAddUregisteredUserDialog"></Button>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>