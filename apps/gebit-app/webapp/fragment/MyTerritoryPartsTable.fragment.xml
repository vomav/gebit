<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   xmlns:form="sap.ui.layout.form">
   
   <Table items="{toPartAssignments}"> 
      <columns>
         <Column>
				<Text text="{i18n>name}"/>
			</Column>
         <Column>
				<Text text="{i18n>isDone}"/>
			</Column>
      </columns>
      <items>
         <ColumnListItem
						type="Inactive"
						press="onPressListItem">
						<cells>
							<Text text="{name}"/>
                     <CheckBox selected="{isDone}"></CheckBox>
						</cells>
			</ColumnListItem>
      </items>
   </Table>
</core:FragmentDefinition>