<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleSortDialogConfirm">
		<sortItems>
			<ViewSettingsItem text="{i18n>name}" key="name" selected="true" />
			<ViewSettingsItem text="{i18n>lastTimeWorked}" key="lastTimeWorked" />
			<ViewSettingsItem text="{i18n>totalCount}" key="totalCount" />
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>