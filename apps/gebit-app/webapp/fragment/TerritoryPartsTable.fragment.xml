<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   xmlns:form="sap.ui.layout.form">
   
   <Table id="territoryPartTable" items="{
         path:'toParts',
         sorter : {
            path : 'name' 
         }
      }"> 
      <columns>
         <Column>
				<Text text="{i18n>name}"/>
			</Column>
         <Column>
				<Text text="{i18n>count}"/>
			</Column>
      </columns>
      <items>
         <ColumnListItem
						type="Inactive"
						press="onPressListItem">
						<cells>
					      <ObjectIdentifier
						      title="{name}"/>
                     <Text text="{count}"/>
						</cells>
			</ColumnListItem>
      </items>
   </Table>
</core:FragmentDefinition>