{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5.gebit.app.reuse.public",
		"type": "application",
		"i18n": "reuse/public/i18n/i18n.properties",
		"title": "JW Gebit",
		"description": "JW Gebit",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "",
		
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/srv.registration/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5.gebit.app.reuse.public.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.112.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.integration": {},
				"sap.uxap": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				  "bundleName": "ui5.gebit.app.i18n.i18n"
				}
			  },
			  
			  "": {
				"dataSource": "mainService",
				"preload": false,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"groupId": "$direct"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "ui5.gebit.app.reuse.public.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true
			},
			"routes": [
				{
					"name": "App",
					"pattern": "",
					"target": "App"
				},
				{
					"name": "welcome",
					"pattern": "welcome",
					"target": "welcome"
				},
				{
					"name": "login",
					"pattern": "welcome/login",
					"target": "login"
				},
				{
					"name": "register",
					"pattern": "welcome/register",
					"target": "register"
				},
				{
					"name": "activate",
					"pattern": "welcome/activate/tenant/{tenantId}/user/{userId}",
					"target": "activate"
				},
				{
					"name": "forgotPassword",
					"pattern": "welcome/forgotPassword",
					"target": "forgotPassword"
				}
			],
			"targets": {
				"app": {
					"type": "View",
					"id": "app",
					"name": "App"
				},
				"login": {
					"type": "View",
					"id": "login",
					"name": "Login"
				},
				"register": {
					"type": "View",
					"id": "register",
					"name": "Register"
				},
				"welcome": {
					"type": "View",
					"id": "welcome",
					"name": "Welcome"
				},
				"activate": {
					"type": "View",
					"id": "activate",
					"name": "Activate"
				},
				"forgotPassword": {
					"type": "View",
					"id": "forgotPassword",
					"name": "ForgotPassword"
				}
			}
		}
	}
}