{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5.gebit.app.reuse.admin",
		"type": "application",
		"i18n": "./i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "",
		
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/srv.globalAdmin/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5.gebit.app.reuse.admin.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.112.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {},
				"sap.f": {},
				"sap.uxap": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				  "bundleName": "ui5.gebit.app.reuse.admin.i18n.i18n"
				}
			  },
			  
			  "": {
				"dataSource": "mainService",
				"preload": false,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"groupId": "$direct"
				}
			},
			"uiModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"ui" : {
						"editMode": false
					}
					
				}
			  }
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "ui5.gebit.app.reuse.admin.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true
			},
			"routes": [
				{
					"name": "App",
					"pattern": "",
					"target": "App"
				},
				{
					"name": "admin",
					"pattern": "admin",
					"target": "admin"
				},
				{
					"name": "users",
					"pattern": "admin/users",
					"target": "users"
				},
				{
					"name": "user",
					"pattern": "admin/users/{userId}",
					"target": "user"
				},
				{
					"name": "userActivations",
					"pattern": "admin/users/{userId}/userActivations/{activationId}",
					"target": "userActivations"
				},
				{
					"name": "tenantMappings",
					"pattern": "admin/users/{userId}/tenantMappings/{tenantMappingId}",
					"target": "tenantMappings"
				}
			],
			"targets": {
	
				"admin": {
					"type": "View",
					"id": "admin",
					"name": "AdminLandingPage"
				},
				"users": {
					"type": "View",
					"id": "users",
					"name": "UsersWorklist"
				},
				"user": {
					"type": "View",
					"id": "user",
					"name": "UserDetail"
				},
				"userAcvtivations": {
					"type": "View",
					"id": "userAcvtivations",
					"name": "UserAcvtivations"
				},
				"tenantMappings": {
					"type": "View",
					"id": "tenantMappings",
					"name": "TenantMappings"
				}
			}
		}
	}
}