<mvc:View
	controllerName="ui5.gebit.app.reuse.admin.controller.AdminLandingPage"
	xmlns:core="sap.ui.core"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:form="sap.ui.layout.form"
			xmlns:f="sap.f"
	>
<Page showHeader="false" id="adminWelcomePage">
		<f:GridContainer  class="sapUiSmallMargin">
			 <f:layout>
				<f:GridContainerSettings rowSize="84px" columnSize="84px" gap="8px" />
			</f:layout>
			<f:layoutXS>
				<f:GridContainerSettings rowSize="70px" columnSize="70px" gap="8px" />
			</f:layoutXS>

			<GenericTile
				press="navigateToUsersWorklist"
				header="{i18n>manageUsers}"
				subheader="{i18n>all}"
				>
				<layoutData>
					<f:GridContainerItemLayoutData minRows="2" columns="2" />
				</layoutData>
				<TileContent footer="">
					<ImageContent src="sap-icon://user-edit" />
				</TileContent>
			</GenericTile>
			
			
		</f:GridContainer>
	</Page>
</mvc:View>
