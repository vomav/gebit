{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5.gebit.app.reuse.territories",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{title}}",
		"description": "{{description}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "",
		
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/srv.searching/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},

	"sap.fiori": {

	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5.gebit.app.reuse.territories.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.112.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.integration": {},
				"sap.uxap": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				  "bundleName": "ui5.gebit.app.reuse.territories.i18n.i18n"
				}
			  },
			  "uiModel": {
				"type": "sap.ui.model.json.JSONModel",
				    "preload": true,
    				"uri": "model/uiModel.json"
			  },
			  
			  "": {
				"dataSource": "mainService",
				"preload": false,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"groupId": "$direct"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "ui5.gebit.app.reuse.territories.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
	
				{
					"name": "worklist",
					"pattern": "territories",
					"target": "territories"
				},
				{
					"name": "detail",
					"pattern": "territory/{id}",
					"target": "territory"
				}
			],
			"targets": {
				"territories": {
					"type": "View",
					"id": "worklist",
					"name": "Worklist"
				},
				"territory": {
					"type": "View",
					"id": "detail",
					"name": "Detail"
				}
			}
		}
	}
}